    <mj-wrapper background-color="#FFF" padding="50px 0px 0px 0px">
      <mj-section padding="0px 0px 15px 0px">
        <mj-column>
          <mj-text mj-class="section-title" font-size="22px" padding="0px">
            In Partnership with:
          </mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="0px 25px 15px 25px" background-color="#FFF">
        <mj-column width="30%" vertical-align="middle">
          <mj-image src="https://f.hubspotusercontent20.net/hubfs/4636824/Emails/Logos/coa_logo.png" width="100px" padding="0"></mj-image>
        </mj-column>
        <mj-column width="70%" vertical-align="middle">
          <mj-image src="https://f.hubspotusercontent20.net/hubfs/4636824/Emails/Logos/APAO--logo.png" width="350px" padding="0"></mj-image>
        </mj-column>
      </mj-section>
    </mj-wrapper>

    <!-- In Partnership with Section -->
    <mj-wrapper background-color="#FFF" padding="30px 0px">
      <mj-section padding="0px 0px 15px 0px">
        <mj-column>
