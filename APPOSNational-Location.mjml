    <!-- Who Should Attend APPOS Section -->
    <mj-wrapper css-class="body-section" background-color="#2F3D4A" padding="50px 25px">
      <mj-section padding="0">
        <mj-column>
          <mj-text mj-class="column-title-light">Who Should Attend APPOS?</mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="0 0 15px 0">
        <mj-column width="35%" border-right="10px solid transparent">
          <mj-text css-class="text-btn-module">Pharmacists</mj-text>
        </mj-column>
        <mj-column width="65%">
          <mj-text css-class="text-btn-module">Nurse Practitioners</mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="0 0 15px 0">
        <mj-column width="65%" border-right="10px solid transparent">
          <mj-text css-class="text-btn-module">Physician Assistants</mj-text>
        </mj-column>
        <mj-column width="35%">
          <mj-text css-class="text-btn-module">Nurses</mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="0">
        <mj-column>
          <mj-text css-class="text-btn-module">Others involved in the care of oncology patients</mj-text>
        </mj-column>
      </mj-section>
      <mj-section padding="30px 0 0 0">
        <mj-column>
          <mj-text align="center" padding="0" text-transform="uppercase" font-weight="600" color="#0B88A8">Offering up to 12 hours of ANCC, ACPE, & CME credit, <br />including pharmacology credit and BCOP credit</mj-text>
        </mj-column>
      </mj-section>
    </mj-wrapper>
    <mj-wrapper padding="0 0 0 0" css-class="body-section">
